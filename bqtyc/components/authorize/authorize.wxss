/* components/authorize/authorize.wxss */
.wrap{
  position: fixed;
  top:0;
  left: 0;
  background: #fff;
  height: 100%;
  width: 100%;
  z-index: 999999999;
}
.logo{
  margin-top:160rpx;
  text-align: center
}

.unlogin{
  margin:60rpx 20px 0;
  background-image: linear-gradient(-90deg, #FFCF02 9%,#FFEA42 95% );
  background-color: none;
  color: #fff !important;
  position: initial;
}
.text{
  margin: 10px 20px;
  font-size: 27rpx;
  color: #555;
}
.title{
  font-size: 20px;
  font-weight: 600;
  text-align: center;
  color: #555;
  margin-top:-10rpx;
}
.text text{
  color: #ffd601;
  font-weight: 500
}