.loading-gif {
  width: 46rpx;
  height: 8rpx;
}
.loading-wrap {
  height: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
  margin-bottom: 20rpx;
  color:#333;
  font-size: 14px;
  }
.loading-wrap image {
  background-color: transparent;
}
.header {
  display: flex;
  align-items: center;
  top: 0;
  position: fixed;
  width: 100%;
  background: #000;
  z-index: 999999999;
}
.header .line {
  height: 36rpx;
}
.header .back, .header .home {
  height: 32rpx;
  width: 40%;
  display: flex;
  line-height: 30rpx;
  align-items: center;
  justify-content: center;
}
.header .title-bar {
  display: flex;
  align-items: center;
  justify-content: space-around;
  width: 138rpx;
  height: 60rpx;
  border: 1rpx solid #FFFFFF;
  border-radius: 32rpx;
  margin-left: 14rpx;
  padding: 0 14rpx;
  position: relative;
  top:-2px;
}


.header .header-title {
  position: absolute;
  left: 50%;
  font-size: 32rpx;
  transform: translateX(-50%);
  color: #fff
}
