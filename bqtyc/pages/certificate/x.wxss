/* pages/certificate/x.wxss */
@import '../../components/area/model.wxss';
page{height: 100%;background-color:#F8F8F8;}
.numType{
  padding:7px 15px;
  height: 34px;
  font-size:14px;
  position: relative
}
.hidden{
  display: none
}
.type-t{
  float: left;
  min-width:65px;
  padding-right:10px;
  color:#495060;
  font-size: 14px;
  line-height: 34px
}
.numType i-icon{
  position: absolute;
  right:10px;
  top:14px;
}
.picker{
   padding-right:10px;
  color:#000;
  font-size: 14px;
  height: 48px;
  line-height: 34px
}
.handleTap .i-cell-text{
  color:#495060;
  padding-left: 7px
}
.areaBtn{
  padding:6px 0;
  min-height:22px;
 line-height:1.6;

}
.i-panel view{
  color: #000 !important;
  font-size: 14px
}
image{
  width: 198rpx;
  height: 139rpx
}
.picWrap{
  padding:7px 15px;
}
.pic-1,.pic-2{
   width: 198rpx;
   text-align: center;
   display: inline-block
}
.pic-1 text,.pic-2 text{
  font-size: 30rpx;
 color: #B2B2B2;
 margin-top: 14rpx;
}
.pic-2{
  margin-left: 60rpx;
}
.btn{
  background: #5CB89A;
  border: 2px solid #5CB89A;
  border-radius: 47px;
  width: 670rpx;
  height:94rpx;
  line-height: 94rpx;
  font-size: 36rpx;
  color: #FFFFFF !important;
  text-align: center;
  margin: 40rpx auto 0
}
.tips{margin-left: 15px;color:#9B9B9B;font-size: 14px}
.tips .icon-b{
  display: inline-block;
  width: 24rpx;
  height: 24rpx;
  border: 1px solid #D0D0D0;
  border-radius: 4px;
  font-size: 26rpx;
  position:relative;
  top:3px;
}
.tips .icon-b i-icon{
  top:-5px;
  left:-3px;
  position: relative
}
.tips text{
  color: #270000;
}
.mt30{margin-top: 30rpx}
