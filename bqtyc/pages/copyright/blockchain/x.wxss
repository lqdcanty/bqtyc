.bottom-bar {
  position: fixed;
  bottom: 0;
  left: 0;
  background-color: #f8f8f8;
  width: 100%;
  font-size: 32rpx;
  line-height: 1.6;
  z-index:100;
}
.custom_btn {
  width: 150px;
  height: 35px;
  line-height: 35px;
  font-size: 14px;
  background-color:#4db798;
  color:#fff;
}
.swiper-item {
  width: 100%;
  border: 0px solid red;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  top: 30px;
}
.banner{
  height: 88px;
  background: #F8F8F8;
}
.content {
  margin-top: 60px;
}
.top-bar {
  position: fixed;
  top: 0;
  left: 0;
  background-color: #000;
  width: 100%;
  font-size: 32rpx;
  line-height: 1.6;
  z-index: 100;
}
.work {
  padding-top: 4px;
  border: 0px solid red;
}
.work .head {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}
.work .head .title {
  color: #fff;
  font-size: 28rpx;
}
.work .remark {
  color: #e0e0e0;
  font-size: 24rpx;
  padding-left:3px;
  padding-right:3px;
  overflow:hidden;
  white-space:nowrap;
  text-overflow:ellipse;
  margin-right: 10px;
}
/*滚动tabs*/
.tabs {
  width: 100%;
  height:36px;
  background-color: #000;
  white-space: nowrap;
}
.tabs .tab {
  width:25%;
  padding-top: 4px;
  color: #fff;
  text-align: center;
  padding-left:0rpx; 
  font-size:28rpx;
  display:inline-block;
  line-height: 33px;
  cursor: pointer;
} 
.tabs .tab .on {
  border-bottom: 4px solid #4db798;
}

.refer_img{
  display: flex;
  align-items: center;
  justify-content: center;
  width:36px;
  height:36px;
  border-radius: 10rpx;
  border: 1px solid #f8f8f8;
}
.copyright {
  display: flex;
  flex-direction: row;
  margin-top: 3px;
  margin-left:3px;
  margin-bottom: 7px;
}

.copyright .icon{
  width: 28px;
  height: 28px;
  display: flex;
  align-items: center;
  justify-content: center;
  color: #1c2438;
  font-size: 28rpx;
  border: 1rpx solid #bbbec4;
  background-color: #dddee1;
}
.copyright .title{
  float: left;
  width: 84px;
  height: 28px;
  display: flex;
  align-items: center;
  justify-content: center;
  color: #666;
  font-size: 24rpx;
  border: 1rpx solid #bbbec4;
  background-color: #dddee1;
}
.copyright image{
  width: 28px!important;
  height: 28px!important;
  display: flex;
  align-items: center;
  justify-content: center;
  color: #1c2438;
  font-size: 28rpx;
  border: 1rpx solid #bbbec4;
  background-color: #dddee1;
}
.copyright .name{
  height: 28px;
  /* display: flex;
  flex-direction: row;
  justify-content: space-between; */
  margin-right: 3px;
  border: 1rpx solid #bbbec4;
  background-color: #F8F8F8;
  border-left: 0rpx solid #bbbec4;
  /* width:100%!important; */
}
.copyright .name input {
  height: 28px;
  color: #1c2438;
  font-family: 'Times New Roman', Times, serif;
  padding-left: 5px;
  padding-right: 5px;
  font-size: 28rpx;
  border: 0rpx solid #bbbec4;
  background-color: #F8F8F8;
}
.copyright .name .right{
  float:right;
  background-color: #F8F8F8;
}
.copyright .name .right .status{
  float:left;
  text-align: center;
  height: 100%;
  font-size: 28rpx;
  color: #80848f;
  padding-left: 5px;
  padding-right: 5px;
  background-color: #F8F8F8;
}
/*滚动tabs*/
.tabs {
  width: 100%;
  height:36px;
  background-color: #000;
  white-space: nowrap;
}
